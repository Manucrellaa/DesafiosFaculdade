programa {
  funcao inicio() {
    real num1, num2

    escreva("digite o primeiro numero: ")
    leia(num1)

    escreva("digite o segundo numero: ")
    leia(num2)

    se (num1 > num2){
      escreva("O numero ", num1, " é maior que o numero ", num2)

    }senao {
      escreva("O numero ", num2, " é maior que o numero ", num1)
    }
    
  }
}
